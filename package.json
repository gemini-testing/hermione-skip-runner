    "hermione-skip-runner"
  ],
  "author": "Anton Usmansky",
  "license": "MIT",
  "dependencies": {
    "gemini-configparser": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-gemini-testing": "^2.8.0",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "sinon": "^6.0.1"
  }
}
